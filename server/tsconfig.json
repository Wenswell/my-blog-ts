{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES2022",
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "@/*": ["./*"],
      "@db/*": ["./db/*"],
      "@router/*": ["./router/*"],
    },
    "outDir": "dist",
    "sourceMap": true,
    "noImplicitAny": true,
  },
  "include": ["src/**/*"],
}